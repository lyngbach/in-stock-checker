{
  "name": "in-stock-checker",
  "version": "1.0.0",
  "description": "Node cron script to check your favorite shopping sites if your desired item is in stock",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-json-modules --no-warnings backend/index.js",
    "dev": "nodemon --exec npm start --ignore client",
    "keys": "./node_modules/.bin/web-push generate-vapid-keys"
  },
  "author": "Rune Lyngbach Jensen",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.2",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.0",
    "puppeteer": "^11.0.0",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
